│"database"   │"entity"     │["_type","_children","id"]│"simulationData"│["_type","_children"]│"controlStructure"│["_type","_children","id"]│"successorStructure"│["_type","_children","id","group"]│"successorStructure"│["_type","_children","id"]        │"successorStructure"│["_type","_children","id"]        │
└─────────────┴─────────────┴──────────────────────────┴────────────────┴─────────────────────┴──────────────────┴──────────────────────────┴────────────────────┴──────────────────────────────────┴────────────────────┴──────────────────────────────────┴────────────────────┴──────────────────────────────────┘


WITH "file:///moon_base.xml" as file_name
CALL apoc.load.xml(file_name)
YIELD value
UNWIND value._children AS tier1
WITH tier1, tier1._children AS schema_outline2
UNWIND schema_outline2 AS tier2
WITH tier1, tier2, tier2._children AS schema_outline3
UNWIND schema_outline3 AS tier3
WITH tier1, tier2, tier3, tier3._children AS schema_outline4
UNWIND schema_outline4 AS tier4
WITH tier1, tier2, tier3, tier4, tier4._children AS schema_outline5
UNWIND schema_outline5 AS tier5
WITH tier1, tier2, tier3, tier4, tier5, tier5._children AS schema_outline6
UNWIND schema_outline6 AS tier6
WITH tier1, tier2, tier3, tier4, tier5, tier6, tier6._children AS schema_outline7
UNWIND schema_outline7 AS tier7
WITH tier1, tier2, tier3, tier4, tier5, tier6, tier7, tier7._children AS schema_outline8
UNWIND schema_outline8 AS tier8

RETURN DISTINCT tier1._type, tier2._type, keys(tier2), tier3._type, keys(tier3), tier4._type, keys(tier4), tier5._type, keys(tier5), tier6._type, keys(tier6), tier7._type, keys(tier7), tier8._type, keys(tier8)

