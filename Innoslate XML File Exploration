WITH "file:///moon_base.xml" as file_name
CALL apoc.load.xml(file_name)
YIELD value
UNWIND value._children AS tier1
WITH tier1 as schema_outline WHERE tier1._type="schema"
WITH schema_outline._children AS schema_outline2
UNWIND schema_outline2 AS tier2
RETURN DISTINCT tier2._type, keys(tier2)

╒═══════════════════╤══════════════════════════╕
│"tier2._type"      │"keys(tier2)"             │
╞═══════════════════╪══════════════════════════╡
│"schemaClass"      │["_type","_children","id"]│
├───────────────────┼──────────────────────────┤
│"schemaRelation"   │["_type","_children","id"]│
├───────────────────┼──────────────────────────┤
│"xmlTargetRelation"│["_type","_children"]     │
└───────────────────┴──────────────────────────┘




WITH "file:///moon_base.xml" as file_name
CALL apoc.load.xml(file_name)
YIELD value
UNWIND value._children AS tier1
WITH tier1 as schema_outline WHERE tier1._type="schema"
WITH schema_outline._children AS schema_outline2
UNWIND schema_outline2 AS tier2
WITH tier2, tier2._children AS schema_outline3
UNWIND schema_outline3 AS tier3

RETURN DISTINCT tier2._type, keys(tier2), tier3._type, keys(tier3)

