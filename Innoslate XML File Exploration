├─────────────┼───────────────────┼──────────────────────────┼───────────────────┼────────────────────────────────────────┤
│"database"   │"entity"           │["_type","_children","id"]│"imageId"          │["_type","_text"]                       │
├─────────────┼───────────────────┼──────────────────────────┼───────────────────┼────────────────────────────────────────┤
│"database"   │"relationship"     │["_type","_children"]     │"sourceId"         │["_type","_text"]                       │
├─────────────┼───────────────────┼──────────────────────────┼───────────────────┼────────────────────────────────────────┤
│"database"   │"relationship"     │["_type","_children"]     │"schemaRelationId" │["_type","_text"]                       │
├─────────────┼───────────────────┼──────────────────────────┼───────────────────┼────────────────────────────────────────┤
│"database"   │"relationship"     │["_type","_children"]     │"targetId"         │["_type","_text"]                       │
├─────────────┼───────────────────┼──────────────────────────┼───────────────────┼────────────────────────────────────────┤
│"database"   │"relationship"     │["_type","_children"]     │"booleanAttribute" │["_type","_children","schemaPropertyId"]│
├─────────────┼───────────────────┼──────────────────────────┼───────────────────┼────────────────────────────────────────┤
│"database"   │"relationship"     │["_type","_children"]     │"numberAttribute"  │["_type","_children","schemaPropertyId"]│
└─────────────┴───────────────────┴──────────────────────────┴───────────────────┴────────────────────────────────────────┘





WITH "file:///moon_base.xml" as file_name
CALL apoc.load.xml(file_name)
YIELD value
UNWIND value._children AS tier1
WITH tier1, tier1._children AS schema_outline2
UNWIND schema_outline2 AS tier2
WITH tier1, tier2, tier2._children AS schema_outline3
UNWIND schema_outline3 AS tier3
WITH tier1, tier2, tier3, tier3._children AS schema_outline4
UNWIND schema_outline4 AS tier4
WITH tier1, tier2, tier3, tier4, tier4._children AS schema_outline5
UNWIND schema_outline5 AS tier5

RETURN DISTINCT tier1._type, tier2._type, keys(tier2), tier3._type, keys(tier3), tier4._type, keys(tier4), tier5._type, keys(tier5)

